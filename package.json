{
  "name": "tyh-ui",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev:docs": "pnpm run dev --filter @tyh-ui/docs",
    "dev:demo": "pnpm run dev --filter @tyh-ui/demo",
    "build:docs": "vite build packages/docs",
    "build:style": "vite build packages/THEME_STYLE",
    "preinstall": "node ./scripts/preinstall.js",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 && pretty-quick --check --branch master",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json && pretty-quick --branch master",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "commit": "cz"
  },
  "license": "ISC",
  "dependencies": {
    "rollup": "^2.70.1",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.2.25"
  },
  "lint-staged": {
    "*.{vue,js,ts,md,json}": "prettier --write"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8",
    "vue-router": "^4.0.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
